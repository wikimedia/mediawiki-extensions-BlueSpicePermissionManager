	"bs-permissionmanager-preset-private-help": "* \u062a\u0646\u0647\u0627 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0648\u0627\u0631\u062f \u0634\u062f\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0645\u062d\u062a\u0648\u0627 \u0631\u0627 '''\u0645\u0634\u0627\u0647\u062f\u0647 \u0648 \u0648\u06cc\u0631\u0627\u06cc\u0634''' \u06a9\u0646\u0646\u062f\n* \u062a\u0646\u0647\u0627 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0631 \u06af\u0631\u0648\u0647 \u00ab\u0645\u062f\u06cc\u0631\u00bb \u0647\u0633\u062a\u0646\u062f \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f '''\u06a9\u0646\u0634\u200c\u0647\u0627\u06cc \u0645\u062f\u06cc\u0631\u06cc\u062a\u06cc''' \u0631\u0627 \u0627\u062c\u0631\u0627 \u06a9\u0646\u0646\u062f",
	"bs-permissionmanager-preset-protected-label": "\u0648\u06cc\u06a9\u06cc \u0645\u062d\u0627\u0641\u0638\u062a\u200c\u0634\u062f\u0647",
	"bs-permissionmanager-preset-protected-help": "* \u0647\u0645\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0645\u062d\u062a\u0648\u0627 \u0631\u0627 '''\u0645\u0634\u0627\u0647\u062f\u0647 \u06a9\u0646\u0646\u062f''' - \u0627\u0632 \u062c\u0645\u0644\u0647 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0646\u0627\u0634\u0646\u0627\u0633\n* \u062a\u0646\u0647\u0627 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0648\u0627\u0631\u062f\u0634\u062f\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0645\u062d\u062a\u0648\u0627 \u0631\u0627 '''\u0648\u06cc\u0631\u0627\u06cc\u0634''' \u06a9\u0646\u0646\u062f\n* \u062a\u0646\u0647\u0627 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0631 \u06af\u0631\u0648\u0647 \u00ab\u0645\u062f\u06cc\u0631\u00bb \u0647\u0633\u062a\u0646\u062f \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f '''\u06a9\u0646\u0634\u200c\u0647\u0627\u06cc \u0645\u062f\u06cc\u0631\u06cc\u062a\u06cc''' \u0631\u0627 \u0627\u062c\u0631\u0627 \u06a9\u0646\u0646\u062f",
	"bs-permissionmanager-preset-public-label": "\u0648\u06cc\u06a9\u06cc \u0639\u0645\u0648\u0645\u06cc",
	"bs-permissionmanager-preset-public-help": "* \u0647\u0645\u0647 \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f \u0645\u062d\u062a\u0648\u0627 \u0631\u0627 '''\u0645\u0634\u0627\u0647\u062f\u0647 \u0648 \u0648\u06cc\u0631\u0627\u06cc\u0634''' \u06a9\u0646\u0646\u062f - \u0627\u0632 \u062c\u0645\u0644\u0647 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0646\u0627\u0634\u0646\u0627\u0633\n* \u062a\u0646\u0647\u0627 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646\u06cc \u06a9\u0647 \u062f\u0631 \u06af\u0631\u0648\u0647 \u00ab\u0645\u062f\u06cc\u0631\u00bb \u0647\u0633\u062a\u0646\u062f \u0645\u06cc\u200c\u062a\u0648\u0627\u0646\u0646\u062f '''\u06a9\u0646\u0634\u200c\u0647\u0627\u06cc \u0645\u062f\u06cc\u0631\u06cc\u062a\u06cc''' \u0631\u0627 \u0627\u062c\u0631\u0627 \u06a9\u0646\u0646\u062f",
	"bs-permissionmanager-switch-preset-confirm": "\u062a\u063a\u06cc\u06cc\u0631 \u067e\u06cc\u0634\u200c\u062a\u0646\u0638\u06cc\u0645\u0627\u062a \u0628\u06cc\u200c\u062f\u0631\u0646\u06af \u0628\u0631 \u062a\u0645\u0627\u0645 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646 \u0627\u06cc\u0646 \u0648\u06cc\u06a9\u06cc \u0627\u0639\u0645\u0627\u0644 \u062e\u0648\u0627\u0647\u062f \u0634\u062f. \u0622\u06cc\u0627 \u0627\u0632 \u0627\u062f\u0627\u0645\u0647\u0654 \u0627\u06cc\u0646 \u06a9\u0627\u0631 \u0645\u0637\u0645\u0626\u0646 \u0647\u0633\u062a\u06cc\u062f\u061f",
	"bs-permissionmanager-preset-save-error": "\u062a\u0646\u0638\u06cc\u0645\u0627\u062a \u0631\u0627 \u0646\u0645\u06cc\u200c\u062a\u0648\u0627\u0646 \u0630\u062e\u06cc\u0631\u0647 \u06a9\u0631\u062f! \u062a\u063a\u06cc\u06cc\u0631\u0627\u062a \u0627\u0639\u0645\u0627\u0644 \u0646\u0634\u062f\u0646\u062f!",
