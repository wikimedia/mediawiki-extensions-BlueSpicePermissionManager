	"bs-permissionmanager-preset-custom-help": "* \u0627\u062e\u062a\u0635\u0627\u0635 \u0646\u0642\u0634\u200c\u0647\u0627 \u0648 \u06af\u0631\u0648\u0647\u200c\u0647\u0627 \u0628\u0647\u200c\u0635\u0648\u0631\u062a \u062f\u0633\u062a\u06cc",
